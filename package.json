{
  "name": "@nuintun/ansi",
  "version": "0.0.3",
  "description": "A pure JavaScript ANSI style parser library.",
  "type": "module",
  "sideEffects": false,
  "main": "cjs/index.cjs",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.cjs",
    "types": "./types/index.d.ts"
  },
  "files": [
    "cjs",
    "esm",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/ansi.git"
  },
  "keywords": [
    "cli",
    "tty",
    "ansi",
    "term",
    "xterm",
    "parser",
    "console",
    "terminal"
  ],
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/ansi/issues"
  },
  "homepage": "https://github.com/nuintun/ansi#readme",
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "scripts": {
    "test": "node tests",
    "lint": "tsc --noEmit",
    "clean": "node tools/clean.js",
    "prepublishOnly": "npm run build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "format": "prettier --write . --ignore-path .prettierignore",
    "build": "npm run clean && npm run build:main && npm run format",
    "build:main": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir types && node tools/fix-types.js"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "magic-string": "^0.30.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  }
}
